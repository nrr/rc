Host *
	CheckHostIP yes
	Ciphers blowfish-cbc,cast128-cbc,arcfour256,arcfour128,arcfour,aes256-ctr,aes256-cbc,aes192-ctr,aes192-cbc

	ForwardAgent yes
	ForwardX11 yes

	PreferredAuthentications publickey,keyboard-interactive,password,gssapi-with-mic,hostbased

	ProxyCommand ssh cloudleaper.corvidae.internal nc -w 10 %h %p

	ServerAliveInterval 5
	TCPKeepAlive yes

	VisualHostKey yes

	IdentityFile ~/.ssh/keys/%h/%r/id_rsa
	IdentityFile ~/.ssh/keys/%h/id_rsa
	IdentityFile ~/.ssh/id_rsa
