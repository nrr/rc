Host *
	CheckHostIP yes
	ForwardX11 yes
	HashKnownHosts yes
	PreferredAuthentications publickey
	ProxyCommand ssh -q -W %h:%p odyssey.corp.corvidae.org
	ServerAliveInterval 5
	TCPKeepAlive yes
	User nrr
	VisualHostKey yes
	IdentityFile ~/.ssh/keys/%h/%r/id_rsa
	IdentityFile ~/.ssh/keys/%h/id_rsa
	IdentityFile ~/.ssh/id_rsa
